    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.46.0",
    "eslint-config-piecioshka": "^2.2.4",
    "eslint-plugin-clean-regex": "^0.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-smells": "^1.0.1",
    "extract-loader": "^5.1.0",
