{
  "private": true,
  "name": "grepnet",
  "version": "1.0.0",
  "description": "Display notification popup when web page is update and grepnet match passed phrase",
  "scripts": {
    "lint": "eslint ./*",
    "build": "webpack --config webpack/webpack.config.dev.js",
    "build:prod": "webpack --config webpack/webpack.config.prod.js",
