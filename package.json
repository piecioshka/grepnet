{
    "private": true,
    "name": "grepnet-client-www",
    "version": "0.1.5",
    "description": "Display notification popup when web page is update and grepnet match passed phrase.",
    "scripts": {
        "clear": "rm -rf node_modules/ app/bower_components/ app/dist/",
        "lint": "eslint ./app/scripts/* --ignore-pattern ./app/scripts/vendor/ ; exit 0",
        "build": "webpack",
        "start": "http-server app/"
    },
    "eslintConfig": {
        "extends": "piecioshka",
        "parser": "babel-eslint",
        "env": {
            "browser": true,
            "amd": true,
            "es6": true
        },
        "globals": {
            "angular": true
        }
    },
    "devDependencies": {
        "babel-core": "^6.1.4",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.5",
        "babel-preset-es2015": "^6.1.4",
        "babel-preset-stage-0": "^6.1.2",
        "eslint": "^8.24.0",
        "eslint-config-piecioshka": "^2.2.4",
        "ng-annotate-webpack-plugin": "^0.3.0",
        "webpack": "^5.74.0"
    },
    "dependencies": {
        "http-server": "^14.1.1"
    }
}
